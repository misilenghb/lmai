{
  "version": "1.0",
  "build": {
    "command": "npm run build:edgeone",
    "outputDirectory": "out",
    "environment": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1"
    },
    "cache": false
  },
  "functions": {
    "runtime": "static"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/admin",
      "destination": "/admin/dashboard",
      "permanent": false
    }
  ]
}
